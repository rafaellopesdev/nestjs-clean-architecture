{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/orit-teste/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/orit-teste/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/orit-teste",
  "projects": {
    "orit-teste": {
      "type": "application",
      "root": "apps/orit-teste",
      "entryFile": "main",
      "sourceRoot": "apps/orit-teste/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orit-teste/tsconfig.app.json"
      }
    },
    "orit-teste-job": {
      "type": "application",
      "root": "apps/orit-teste-job",
      "entryFile": "main",
      "sourceRoot": "apps/orit-teste-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orit-teste-job/tsconfig.app.json"
      }
    }
  }
}